<h2>Log in</h2>
<div class="panel">
<%= form_for(resource, as: resource_name, url: session_path(resource_name), auto_labels: false) do |f| %>
  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true %>
  </div>

  <div class="field">
    <%= f.label :password %><br />
    <%= f.password_field :password, autocomplete: "off" %>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="field">
      <%= f.check_box :remember_me, label: false %>
      <%= f.label :remember_me %>
    </div>
  <% end -%>

  <div class="actions">
    <%= f.submit "Log in", class: 'button small success' %>
    <%= render "devise/shared/links" %>
  </div>
<% end %>

</div>
